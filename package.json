      "@types/signale": "^1.4.1",
      "@types/supertest": "^2.0.11",
      "@types/swagger-jsdoc": "^6.0.0",
      "@types/swagger-ui-express": "^4.1.2",
      "@types/uuid": "^8.3.0",
      "@types/validator": "^10.11.2",
      "@typescript-eslint/eslint-plugin": "^4.15.1",
      "@typescript-eslint/parser": "^4.15.1",
      "concurrently": "^6.0.0",
